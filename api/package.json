{
  "name": "@gear-js/api",
  "version": "0.23.4",
  "description": "A JavaScript library that provides functionality to connect GEAR Component APIs.",
  "main": "cjs/index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "author": "Gear Technologies",
  "bugs": {
    "url": "https://github.com/gear-tech/gear-js/issues"
  },
  "homepage": "https://github.com/gear-tech/gear-js/tree/master/api#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gear-tech/gear-js.git"
  },
  "keywords": [
    "gear",
    "gear-js",
    "gear-api"
  ],
  "scripts": {
    "test": "jest --runInBand",
    "prebuild": "rm -rf lib",
    "build": "rollup --config rollup.config.js",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint --fix ./src --ext .ts"
  },
  "exports": {
    "./cjs/package.json": "./cjs/package.json",
    "./cjs/*": "./cjs/*.js",
    ".": {
      "import": "./index.js",
      "require": "./cjs/index.js",
      "types": "./index.d.ts"
    },
    "./types": {
      "import": "./types/index.d.ts"
    }
  },
  "license": "GPL-3.0",
  "dependencies": {
    "@polkadot/api": "^8.11.3",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "7.18.4",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-typescript": "7.17.12",
    "@rollup/plugin-commonjs": "22.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-typescript": "8.3.3",
    "@types/jest": "27.5.2",
    "@types/rollup-plugin-peer-deps-external": "2.2.1",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-jest": "28.1.1",
    "eslint": "8.19.0",
    "jest": "28.1.1",
    "rollup": "2.75.6",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-typescript2": "0.31.2",
    "ts-jest": "28.0.4",
    "typescript": "4.7.3"
  }
}