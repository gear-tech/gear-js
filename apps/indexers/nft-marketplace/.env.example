NFT_META=/path/to/nft.meta.wasm

# url of subsquid archive
ARCHIVE_URL=https://gear-testnet.archive.subsquid.io/graphql
# block from which to start indexing
FROM_BLOCK=2057000

NODE_TLS_REJECT_UNAUTHORIZED=0
