{
  "name": "@gear-js/wasm-compiler",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "prebuild": "rm -rf build",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "watch": "ts-node-dev src/main.ts"
  },
  "dependencies": {
    "adm-zip": "0.5.10",
    "dockerode": "3.3.3",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "multer": "1.4.5-lts.1",
    "pg": "8.7.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.6",
    "typeorm": "0.3.7"
  },
  "devDependencies": {
    "@types/adm-zip": "0.5.0",
    "@types/dockerode": "3.3.9",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/multer": "1.4.7",
    "@types/node": "17.0.38",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "3.4.1",
    "prettier": "2.7.1",
    "ts-loader": "8.4.0",
    "ts-node": "9.1.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
