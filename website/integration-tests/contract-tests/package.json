{
  "name": "@gear-js/contract-tests",
  "version": "0.0.1",
  "description": "automated tests for apis",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": [
    "tests"
  ],
  "author": "me",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typeorm": "^0.2.41"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": "$",
    "transform": {
      "^.+\\.[tj]s$": "ts-jest"
    },
    "setupFiles": [
      "reflect-metadata"
    ],
    "collectCoverageFrom": [
      "**/*.[tj]s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
