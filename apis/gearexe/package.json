{
  "name": "gearexe",
  "private": true,
  "version": "0.0.1",
  "description": "Typescript library for interacting with Gear.exe",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "./scripts/run-tests.sh",
    "build": "npx rollup -c",
    "clean": "rm -rf lib",
    "abi": "node scripts/convert-abi.mjs"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "peerDependencies": {
    "ethers": "^6.14"
  },
  "devDependencies": {
    "@gear-js/api": "0.42.0",
    "@polkadot/api": "16.0.1",
    "ethers": "6.14.3",
    "gear-js-util": "workspace:^",
    "kzg-wasm": "0.5.0",
    "sails-js": "0.4.1",
    "sails-js-parser": "0.4.1"
  }
}
